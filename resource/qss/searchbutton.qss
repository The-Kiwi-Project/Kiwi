QPushButton{
	font: 12pt "Calibri Light";
    color: #2f3640;
    background-color: #f3f5f7;
    border-radius: 4px;
    border-width: 0px;
    padding: 5px;
    width: 40px;
    height: 40px;
}
QPushButton:hover{	
    background-color: #eaf0f4;
}
QPushButton:pressed,QPushButton:checked{
    background-color: #ddeef6;
}
QPushButton:disabled{
    background-color: #f3f5f7;
}