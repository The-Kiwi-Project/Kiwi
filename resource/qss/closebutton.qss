QPushButton{
	font: 12pt "Calibri Light";
    color: #2f3640;
    background-color: #f7e8e8;
    border-radius: 4px;
    border-width: 0px;
    padding: 5px;
    width: 40px;
    height: 40px;
}
QPushButton:hover{	
    background-color: #f7e4e4;
}
QPushButton:pressed,QPushButton:checked{
    background-color: #f8d5d5;
}
QPushButton:disabled{
    background-color: #f6ebeb;
}